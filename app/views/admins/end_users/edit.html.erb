<div class="container">
	<div class="row">
		<h3>会員編集画面</h3>
		<%= form_with model: @end_user, url: admins_end_user_path(@end_user), local: true do |f| %>
			<table>
				<thead>
					<tr>
						<th colspan="2"></th>
					</tr>
				</thead>
				<tbody>
					<tr>
						<td>会員ID</td>
						<td><%= @end_user.id %></td>
					</tr>
					<tr>
						<td>氏名</td>
						<td><%= f.text_field :first_name %>  <%= f.text_field :last_name %></td>
					</tr>
					<tr>
						<td>フリガナ</td>
						<td><%= f.text_field :first_kana_name %>  <%= f.text_field :last_kana_name %></td>
					</tr>
					<tr>
						<td>郵便番号</td>
						<td><%= f.text_field :postcode %></td>
					</tr>
					<tr>
						<td>住所</td>
						<td><%= f.text_field :address %></td>
					</tr>
					<tr>
						<td>電話番号</td>
						<td><%= f.text_field :phone_number %></td>
					</tr>
					<tr>
						<td>メールアドレス</td>
						<td><%= f.email_field :email %></td>
					</tr>
					<tr>
						<td>会員ステータス</td>
						<td><%= f.radio_button :is_deleted, "有効" %><%= f.label :is_deleted, "有効" %></td>
						<td><%= f.radio_button :is_deleted, "退会済み" %><%= f.label :is_deleted, "退会済み" %></td>
					</tr>
					<tr>
						<td><%= f.submit '変更を保存する' %></td>
					</tr>
				</tbody>
			</table>
		<% end %>
	</div>
</div>