<div class="container">
	<div class="row">
		<h3>会員一覧画面</h3>
		<table class="table">
			<thead>
				<tr>
					<th>会員ID</th>
					<th>氏名</th>
					<th>アドレス</th>
					<th>ステータス</th>
					<th colspan="4"></th>
				</tr>
			</thead>
			<tbody>
				<% @end_users.each do |end_user| %>
					<tr>
						<td><%= end_user.id %></td>
						<td><%= link_to end_user.full_name, admins_end_user_path(end_user) %></td>
						<td><%= end_user.email %></td>
						<td><%= end_user.is_deleted %></td>
					</tr>
				<% end %>
			</tbody>
		</table>
	</div>
</div>